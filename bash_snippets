# A function the returns it's name and line number
function a() {
        echo "function $FUNCNAME, line $LINENO"
}

# Env variables
EPOCHSECONDS
EUID
FUNCNAME
GROUPS
LINENO
RANDOM

# References
a=a
declare -n b=a
b is now a reference to a

# Arrays
declare -a array_a
declare -b array_b
array_a=(a)
array_b+=($a{[@]})
